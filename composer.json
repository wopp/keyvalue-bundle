{
    "name": "wopp/keyvalue-bundle",
    "description": "KeyValue bundle for Symfony2",
    "keywords": ["keyvalue", "redis", "mongodb"],
    "homepage": "https://github.com/wopp-me/keyvalue-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Sergio Arroyo Cuevas",
            "email": "serch@wopp.me"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.0,<2.3-dev",
        "doctrine/mongodb-odm-bundle": ">=v3.0.0-BETA1",
        "doctrine/mongodb-odm": ">=1.0.0-BETA1",
        "predis/predis": ">=v0.8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.7.2"
    },
    "autoload": {
        "psr-0": { "Wopp\\KeyValueBundle": "" }
    },
    "target-dir": "Wopp/KeyValueBundle",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}